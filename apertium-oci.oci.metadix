    <e lm="nuòch"><p><l>nuòch</l>   <r>nuèch</r></p><par n="v:ue_uo"/><par n="p:nom_feminin"/></e>

    <e lm="nuèch"><i>nuè</i><par n="a:ch_it/nom_feminin"/></e>
