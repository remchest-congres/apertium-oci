    <!-- <e lm="nuèch"><i>n</i><par n="p:ue_uo"/><par n="p:ch_it"/></e> -->

    <e lm="lièch"><i>liè</i><par n="p:ch_it"/><par n="p:nom_masculin"/></e>

    <e lm="fuelha"><par n="p:f_h"/><i>uelha</i><par n="p:nom_feminin"/></e>
    <e lm="farina"><par n="p:f_h"/><i>ari</i><par n="p:casuda_n"/><i>a</i><par n="p:nom_feminin"/></e>
  </section>
